                ShowOnMenu="HideIfNoCmdLine"
                Deletable="0"
                SaveOption="SaveNone">
                <Exec
                    CmdLine="gnucoptions"
                    Type="Slick-C"/>
            </Target>
        </Menu>
        <List Name="GNUC Options">
            <Item
                Name="LinkerOutputType"
                Value="SharedLibrary"/>
        </List>
        <Includes/>
        <Libs PreObjects="0"/>
    </Config>
    <Files>
        <Folder
            Name="Source Files"
            Filters="*.c;*.C;*.cc;*.cpp;*.cp;*.cxx;*.c++;*.prg;*.pas;*.dpr;*.asm;*.s;*.bas;*.java;*.cs;*.sc;*.e;*.cob;*.html;*.rc;*.tcl;*.py;*.pl;*.d">
            <F N="BufferedFile.cpp"/>
            <F N="IDBDataFile.cpp"/>
            <F N="IDBFactory.cpp"/>
            <F N="IDBFileSystem.cpp"/>
            <F N="IDBLogger.cpp"/>
            <F N="IDBPolicy.cpp"/>
            <F N="PosixFileSystem.cpp"/>
            <F N="tdriver.cpp"/>
            <F N="UnbufferedFile.cpp"/>
        </Folder>
        <Folder
            Name="Header Files"
            Filters="*.h;*.H;*.hh;*.hpp;*.hxx;*.inc;*.sh;*.cpy;*.if">
            <F N="BufferedFile.h"/>
            <F N="BufferedFileFactory.h"/>
            <F N="FileFactoryBase.h"/>
            <F N="IDBDataFile.h"/>
            <F N="IDBFactory.h"/>
            <F N="IDBFileSystem.h"/>
            <F N="IDBLogger.h"/>
            <F N="IDBPolicy.h"/>
