[Unit]
Description=MariaDB Columnstore
After=network.target
[Service]
Type=forking
#Restart=on-abnormal
GuessMainPID=yes
ExecStart=@CMAKE_INSTALL_PREFIX@/bin/columnstore start
ExecStopPost=@CMAKE_INSTALL_PREFIX@/bin/columnstore stop
TimeoutStopSec=5
LimitNOFILE=65535
[Install]
WantedBy=multi-user.target
