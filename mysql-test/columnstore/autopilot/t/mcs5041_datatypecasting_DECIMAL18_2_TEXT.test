select dtt1.cidx, dtt1.CDECIMAL18_2, dtt2.CTEXT from datatypetestm1 dtt1, datatypetestm2 dtt2 where dtt1.cidx = dtt2.cidx and dtt1.CDECIMAL18_2 > dtt2.CTEXT ;
select dtt1.cidx, dtt1.CDECIMAL18_2, dtt2.CTEXT from datatypetestm1 dtt1, datatypetestm2 dtt2 where dtt1.cidx = dtt2.cidx and dtt1.CDECIMAL18_2 >= dtt2.CTEXT ;
select dtt1.cidx, dtt1.CDECIMAL18_2, dtt2.CTEXT from datatypetestm1 dtt1, datatypetestm2 dtt2 where dtt1.cidx = dtt2.cidx and dtt1.CDECIMAL18_2 <> dtt2.CTEXT ;
select dtt1.cidx, dtt1.CDECIMAL18_2, dtt2.CTEXT from datatypetestm1 dtt1, datatypetestm2 dtt2 where dtt1.cidx = dtt2.cidx and dtt1.CDECIMAL18_2 < dtt2.CTEXT ;
select dtt1.cidx, dtt1.CDECIMAL18_2, dtt2.CTEXT from datatypetestm1 dtt1, datatypetestm2 dtt2 where dtt1.cidx = dtt2.cidx and dtt1.CDECIMAL18_2 <= dtt2.CTEXT ;
