CREATE TABLE t1 (a DECIMAL(30,1));
INSERT INTO t1 VALUES (1.4),(1.5);
SELECT TRUNCATE(1.2345, a) FROM t1;
DROP TABLE t1;


CREATE TABLE t1 (a DECIMAL(10,1));
INSERT INTO t1 VALUES (15.4),(15.5),(16.0);
