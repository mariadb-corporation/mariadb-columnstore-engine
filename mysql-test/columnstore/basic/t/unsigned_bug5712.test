(2147483647),
(2147483648);

SELECT LEFT(time_tk,6) AS time_tk_left FROM bug5712;
SELECT RIGHT(time_tk,6) AS time_tk_right FROM bug5712;

DROP TABLE IF EXISTS bug5712;

# Clean UP
DROP DATABASE unsigned_bug5712_db;
