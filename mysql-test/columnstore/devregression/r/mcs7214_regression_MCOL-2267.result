from `MCOL-2267b` a,cte1 b 
where a.my_id=b.my_id 
group by c1 
having sum1<1 and sum2<1 and total>0 ),
cte3 as (select a.c1,
group_concat(abs((2*b.type-a.type)-a.type) order by b.f1 separator '') as gc1,
