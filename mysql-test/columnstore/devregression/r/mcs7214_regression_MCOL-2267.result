cte4 as (select a.c1,
replace(replace(group_concat((2*b.type-a.type)-1 order by b.f1 separator ''),'-2','0'),'-1','1') as gc3,
group_concat(a.type order by b.f1 separator '') as gc4,
sum1,
sum2              
from `MCOL-2267b` a,cte1 b,cte2 c   
